{
  "name": "boilerify-react-redux",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "test": "mocha --recursive tests",
    "start": "npm run build:pro && pm2 start ./scripts/www",
    "develop": "npm run build:dev && nodemon ./scripts/www",
    "build:dev": "simplifyify ./client/main.js -d -o ./public/bundle.js",
    "build:pro": "simplifyify ./client/main.js -d -m -o ./public/bundle.min.js"
  },
  "dependencies": {
    "body-parser": "1.16.0",
    "cookie-parser": "1.4.3",
    "debug": "2.6.0",
    "express": "4.14.1",
    "morgan": "1.7.0",
    "pm2": "2.4.2",
    "pug": "2.0.0-beta10",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-redux": "5.0.3",
    "redux": "3.6.0",
    "serve-favicon": "2.3.2"
  },
  "devDependencies": {
    "babel-preset-es2015": "6.22.0",
    "babel-preset-es2016": "6.22.0",
    "babel-preset-es2017": "6.22.0",
    "babel-preset-react": "6.23.0",
    "babelify": "7.3.0",
    "browserify-css": "0.10.0",
    "chai": "3.5.0",
    "eslint": "3.16.1",
    "jsdom": "9.11.0",
    "mocha": "3.2.0",
    "nock": "9.0.9",
    "nodemon": "1.11.0",
    "simplifyify": "3.2.1",
    "supertest": "3.0.0"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "react",
            "es2015",
            "es2016",
            "es2017"
          ]
        }
      ]
    ]
  }
}
